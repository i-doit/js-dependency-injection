{
    "name": "js-dependency-injection",
    "version": "1.0.0",
    "description": "Dependency Injection container inspired by Symfony DI",
    "main": "./lib/index.js",
    "module": "es/index.js",
    "files": [
        "dist",
        "lib",
        "src",
        "es"
    ],
    "scripts": {
        "build:commonjs": "NODE_ENV=production cross-env BABEL_ENV=commonjs babel src --out-dir lib",
        "build:es": "babel src --out-dir es",
        "build": "npm run build:commonjs && npm run build:es",
        "clean": "rimraf lib dist es coverage",
        "prepare": "npm run clean && npm run build",
        "lint": "eslint \"./src/**/*.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/i-doit/js-dependency-injection.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-syntax-export-default-from": "^7.12.1",
        "@babel/preset-env": "^7.11.5",
        "babel-eslint": "^10.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.13.0",
        "eslint-plugin-import": "^2.22.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.3.1",
        "@rollup/plugin-babel": "^5.2.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.3.3",
        "rollup-plugin-terser": "^7.0.2"
    },
    "keywords": [
        "DI",
        "dependency",
        "injection",
        "dependency injection",
        "javascript",
        "container"
    ],
    "author": "Pavel Abduramanov <pabduramanov@i-doit.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/i-doit/js-dependency-injection/issues"
    },
    "homepage": "https://github.com/i-doit/js-dependency-injection#readme"
}
